{
  "name": "quicksites-core",
  "version": "1.0.0",
  "description": "Modular site generator for local business templates with Supabase integration.",
  "type": "module",
  "prettier": "./prettier.config.cjs",
  "main": "init.js",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "quickpush": "./quickpush.sh",
    "fonts": "ts-node scripts/download-google-fonts.ts",
    "hosts:add": "ts-node scripts/manage-hosts-from-supabase.ts add",
    "hosts:remove": "ts-node scripts/manage-hosts-from-supabase.ts remove",
    "dev": "node scripts/dev-motd.js && next dev --hostname 0.0.0.0",
    "build": "next build",
    "build:prod": "rm -rf .next;NODE_OPTIONS='--max-old-space-size=8192' next build",
    "start": "next start",
    "clean": "rm -rf .next node_modules/.cache .turbo logs/deploys.db",
    "reset": "npm run clean && npm install && npm run dev",
    "test": "playwright test",
    "test:visual": "playwright test tests/visual-regression.spec.ts",
    "test:routes": "ts-node scripts/test-routes.ts",
    "generate:sitemap": "ts-node scripts/generate-sitemap.ts",
    "visualize:routes": "ts-node scripts/visualize-routes.ts",
    "check:casing": "ts-node scripts/check-import-casing.ts",
    "cleanup:screenshots": "ts-node scripts/cleanup-screenshots.ts",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "lint:links": "node scripts/check-links.js",
    "lint:errors:rules": "eslint . --format json --max-warnings=0 | jq -r '.[] | select(.errorCount > 0) | .filePath as $file | .messages[] | select(.severity == 2) | \"\\($file):\\(.line):\\(.column) — \\(.message) [\\(.ruleId)]\"'",
    "build:lint": "esbuild scripts/lint-digest.ts --outfile=.lint-tmp/scripts/lint-digest.js --platform=node --target=node20",
    "lint:report": "echo ✅ Step 1: generate the json report && rm -f lint-report.json lint-summary.txt lint-summary.md && eslint . --ext .ts,.tsx,.js,.jsx --format json --output-file lint-report.json && tsx scripts/run-lint-report.ts",
    "lint:digest": "echo ✅ Step 2: summarize the json report && tsx scripts/lint-digest.ts",
    "preflight": "npm run lint:err && npm run typecheck:err && npm run build",
    "typecheck:err": "tsc --noEmit --pretty false 2>&1 | grep -i 'error'",
    "lint:err": "eslint . --ext .ts,.tsx --quiet",
    "format": "prettier . --write",
    "typecheck": "tsc --noEmit",
    "commit": "cz",
    "inspect:query": "tsx scripts/query-inspector.ts",
    "docker:build": "docker-compose -f docker-compose.prod.yml up --build -d",
    "pm2:start": "pm2 start ecosystem.config.js",
    "openapi:gen": "tsx scripts/gen-openapi.ts",
    "openapi:validate": "openapi lint openapi.json",
    "openapi:build": "npm run openapi:gen && npm run openapi:validate",
    "prepare": "husky install",
    "screenshot:run": "tsx scripts/screenshot-worker.ts"
  },
  "scripts-meta": {
    "dev": "Start the development server",
    "build": "Build for production",
    "start": "Run the production server",
    "clean": "Wipe all build + temp artifacts",
    "reset": "Full reset of dev environment",
    "test": "Run all Playwright tests",
    "test:visual": "Run visual regression tests",
    "test:routes": "Test live route responses",
    "generate:sitemap": "Create public sitemap.xml",
    "visualize:routes": "Print route tree",
    "cleanup:screenshots": "Clean old screenshot folders",
    "lint": "Run ESLint on TypeScript files",
    "lint:links": "Check for broken links",
    "format": "Auto-format codebase",
    "typecheck": "Run TypeScript without emitting files",
    "prepare": "Hook setup for git",
    "commit": "Use Commitizen for conventional commits",
    "inspect:query": "Custom Zod/Query inspector",
    "docker:build": "Build & run container using Docker Compose",
    "pm2:start": "Run production server via PM2"
  },
