{
  "extends": "./tsconfig.base.json",
  "include": [
    ".next/types",
    "_pages-legacy",
    "app",
    "components",
    "hooks",
    "lib",
    "next-env.d.ts",
    "pages",
    "scripts",
    "types",
    ".next/types/**/*.ts",
    "admin",
    "admin/types",
    "admin/components",
    "admin/lib",
    "admin/hooks",
    "admin/pages",
    "admin/lib/supabase",
    "admin/lib/supabase/serverClient"
  ],
  "exclude": [
    "node_modules",
    ".next"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "tsBuildInfoFile": "./.tsbuildinfo",
    "outDir": ".lint-tmp",
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
    "types": [
      "next",
      "next/image-types/global",
      "node"
    ],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "allowJs": true,
    "isolatedModules": true
  }
}
